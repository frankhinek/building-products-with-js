{
  "name": "experts-server",
  "version": "0.1.0",
  "description": "Experts REST backend",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "db:init": "docker run -d -p 28015:28015 -p 8090:8080 -v $PWD/db:/data --name expertsdb rethinkdb",
    "db:start": "docker start expertsdb",
    "db:stop": "docker stop expertsdb",
    "db:rm": "docker rm expertsdb",
