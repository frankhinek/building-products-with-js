  },
  "homepage": "https://github.com/frankhinek/building-products-with-js#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015-node": "^6.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
